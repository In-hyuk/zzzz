        <tr>
            <td><strong>X</strong></td>
            <% for(int i=2; i<=9; i++) { %>
                <td><strong><%= i %></strong></td>
            <% } %>
        </tr>
        
        <% for(int j=1; j<=9; j++) { %>
            <tr>
                <td><%= j %></td>
                <% for(int i=2; i<=9; i++) { %>
                    <td><%= i * j %></td>
                <% } %>
            </tr>
        <% } %>
    </table>
